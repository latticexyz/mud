{
  "name": "@latticexyz/utils",
  "version": "1.42.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "src/index.ts",
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup",
    "clean": "pnpm run clean:js",
    "clean:js": "rimraf dist",
    "dev": "tsup --watch",
    "docs": "rimraf API && typedoc src && find API -type f -name '*.md' -exec sed -E -i \"\" \"s/(#.*)(<.*>)/\\1/\" {} \\;",
    "lint": "eslint . --ext .ts",
    "release": "npm publish --access=public",
    "test": "tsc --noEmit && jest"
  },
  "dependencies": {
    "ethers": "^5.7.2",
    "mobx": "^6.7.0",
    "proxy-deep": "^3.1.1",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0",
    "typedoc": "0.23.21",
    "typedoc-plugin-markdown": "^3.13.6"
  },
  "peerDependencies": {
    "ethers": "^5.7.2",
    "mobx": "^6.7.0",
    "proxy-deep": "^3.1.1",
    "rxjs": "^7.5.5",
    "web3-utils": "^1.8.0"
  },
  "gitHead": "914a1e0ae4a573d685841ca2ea921435057deb8f"
