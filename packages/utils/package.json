{
  "name": "@latticexyz/utils",
  "license": "MIT",
  "version": "1.32.0",
  "type": "module",
  "source": "src/index.ts",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/utils"
  },
  "scripts": {
    "prepare": "yarn build",
    "lint": "eslint . --ext .ts",
    "test": "tsc && jest",
    "link": "yarn link",
    "docs": "rimraf API && typedoc src && find API -type f -name '*.md' -exec sed -E -i \"\" \"s/(#.*)(<.*>)/\\1/\" {} \\; && echo 'label: API' > API/index.yml",
    "prepack": "mv package.json package.json.bak && jq \".main = \\\"dist/index.js\\\"\" package.json.bak > package.json",
    "postpack": "mv package.json.bak package.json || echo 'no package.json.bak'",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "release": "npm publish || echo 'version already published'"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.1",
    "buffer-lite": "^1.0.0",
    "ethers": "^5.6.6",
    "jest": "^27.5.1",
    "mobx": "^6.5.0",
    "proxy-deep": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.69.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rxjs": "^7.5.5",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typedoc": "0.23.21",
    "typescript": "^4.5.5",
    "web3-utils": "^1.8.0"
  },
  "peerDependencies": {
    "ethers": "^5.6.6",
    "mobx": "^6.5.0",
    "proxy-deep": "^3.1.1",
    "rxjs": "^7.5.5",
    "web3-utils": "^1.8.0"
  },
  "gitHead": "218f56893d268b0c5157a3e4c603b859e287a343",
  "dependencies": {
    "typedoc-plugin-markdown": "^3.13.6"
  }
}
