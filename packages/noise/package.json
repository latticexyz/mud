  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@types/chai": "^4.3.5",
    "@types/node-fetch": "^2.6.2",
    "assemblyscript": "^0.21.2",
    "chai": "^4.3.6",
    "ethers": "^5.7.2",
    "hardhat": "^2.10.2",
    "keccak-wasm": "^0.10.3",
    "tsup": "^6.7.0",
    "web3-utils": "^1.8.0"
  },
