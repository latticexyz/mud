{
  "name": "@latticexyz/schema-type",
  "version": "2.2.21",
  "description": "SchemaType enum for various languages",
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/schema-type"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    },
    "./internal": {
      "import": {
        "import": "./dist/internal.js",
        "types": "./dist/internal.d.ts"
      },
      "require": {
        "require": "./dist/internal.cjs",
        "types": "./dist/internal.d.cts"
      }
    },
    "./deprecated": {
      "import": {
        "import": "./dist/deprecated.js",
        "types": "./dist/deprecated.d.ts"
      },
      "require": {
        "require": "./dist/deprecated.cjs",
        "types": "./dist/deprecated.d.cts"
      }
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "internal": [
        "./dist/internal.d.ts"
      ],
      "deprecated": [
        "./dist/deprecated.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "out",
    "src"
  ],
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup",
    "clean": "pnpm run clean:js",
    "clean:js": "shx rm -rf dist/typescript",
    "dev": "tsup --watch",
    "gas-report": "gas-report --save gas-report.json",
    "test": "vitest --run && forge test",
    "test:ci": "pnpm run test"
  },
  "dependencies": {
    "abitype": "1.0.6"
  },
  "devDependencies": {
    "@latticexyz/gas-report": "workspace:*",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "viem": "2.23.2"
  },
  "peerDependencies": {
    "viem": "2.x"
  }
}
