{
  "name": "@latticexyz/config",
  "version": "2.0.9",
  "description": "Config for Store and World",
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/config"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./internal": "./dist/internal.js",
    "./library": "./dist/deprecated/library.js",
    "./register": "./dist/deprecated/register.js",
    "./node": "./dist/deprecated/node.js"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./src/exports/index.ts"
      ],
      "internal": [
        "./src/exports/internal.ts"
      ],
      "library": [
        "./src/deprecated/library/index.ts"
      ],
      "register": [
        "./src/deprecated/register/index.ts"
      ],
      "node": [
        "./src/deprecated/node/index.ts"
      ]
    }
  },
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup",
    "clean": "pnpm run clean:js",
    "clean:js": "rimraf dist",
    "dev": "tsup --watch",
    "test": "tsc --noEmit",
    "test:ci": "pnpm run test"
  },
  "dependencies": {
    "@latticexyz/common": "workspace:*",
    "@latticexyz/schema-type": "workspace:*",
    "esbuild": "^0.17.15",
    "find-up": "^6.3.0",
    "viem": "2.9.20",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.0"
  },
  "devDependencies": {
    "tsup": "^6.7.0"
  }
}
