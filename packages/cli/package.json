{
  "name": "@latticexyz/cli",
  "version": "1.42.0",
  "description": "Command line interface for mud",
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "mud": "./dist/mud.js",
    "mud2": "./dist/mud2.js"
  },
  "scripts": {
    "build": "vite build",
    "codegen": "tsx ./scripts/codegen.ts",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts",
