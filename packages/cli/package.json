{
  "name": "@latticexyz/cli",
  "version": "0.2.0",
  "description": "Command line interface for mud",
  "main": "./build/cli.js",
  "license": "MIT",
  "bin": {
    "mud": "./build/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "prepare": "chmod u+x build/cli.js",
    "globalinstall": "yarn build && yarn prepare && yarn link && yarn global add @latticexyz/cli@link:./",
    "build": "rimraf build && tsc -p . && yarn prepare",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts",
    "link:bin": "export PATH=\"$(yarn global bin):$PATH\""
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^17.0.34",
    "@types/openurl": "^1.0.0",
    "@types/yargs": "^17.0.10",
    "nodemon": "^2.0.16",
    "pkg": "^5.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@latticexyz/utils": "^0.2.0",
    "chalk": "^5.0.1",
    "clear": "^0.1.0",
    "commander": "^9.2.0",
    "esm": "^3.2.25",
    "ethers": "^5.6.7",
    "execa": "^6.1.0",
    "figlet": "^1.5.2",
    "glob": "^8.0.3",
    "inquirer": "^8.2.4",
    "inquirer-prompt-suggest": "^0.1.0",
    "listr2": "^4.0.5",
    "netlify": "^11.0.1",
    "node-fetch": "^3.2.6",
    "openurl": "^1.1.1",
    "path": "^0.12.7",
    "uuid": "^8.3.2",
    "yargs": "^17.5.1"
  },
  "pkg": {
    "scripts": "build/**/*.js"
  },
  "gitHead": "2000994f695cee35fcc32558f399584922073016"
}
