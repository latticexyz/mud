{
  "name": "@latticexyz/faucet",
  "version": "2.2.14",
  "description": "Faucet API for Lattice testnet",
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/faucet"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "faucet-server": "./bin/faucet-server.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:js",
    "build:js": "tsup",
    "clean": "pnpm run clean:js",
    "clean:js": "shx rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "start": "tsx bin/faucet-server",
    "test": "tsc --noEmit",
    "test:ci": "pnpm run test"
  },
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/cors": "^8.3.0",
    "@latticexyz/common": "workspace:*",
    "@trpc/client": "10.34.0",
    "@trpc/server": "10.34.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "fastify": "^4.21.0",
    "viem": "catalog:",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "tsup": "^6.7.0",
    "vitest": "0.34.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
