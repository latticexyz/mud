{
  "name": "@latticexyz/abi-ts",
  "version": "2.2.2",
  "description": "Create TypeScript type declaration files (`.d.ts`) for your ABI JSON files.",
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/abi-ts"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "abi-ts": "./bin/abi-ts.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "shx rm -rf dist",
    "dev": "tsup --watch",
    "test": "vitest typecheck --run --passWithNoTests && vitest --run --passWithNoTests",
    "test:ci": "pnpm run test"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "execa": "^7.0.0",
    "glob": "^10.4.2",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^18.15.11",
    "@types/yargs": "^17.0.10",
    "tsup": "^6.7.0",
    "vitest": "0.34.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
