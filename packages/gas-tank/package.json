{
  "name": "@latticexyz/gas-tank",
  "version": "2.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/latticexyz/mud.git",
    "directory": "packages/gas-tank"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./mud.config": "./dist/mud.config.js",
    "./worlds.json": "./worlds.json",
    "./out/*": "./out/*"
  },
  "typesVersions": {
    "*": {
      "worlds.json": [
        "./worlds.json"
      ],
      "mud.config": [
        "./mud.config.ts"
      ]
    }
  },
  "scripts": {
    "build": "pnpm build:js && pnpm build:mud",
    "build:js": "tsup",
    "build:mud": "mud build",
    "clean": "pnpm run clean:abi && pnpm run clean:js && pnpm run clean:mud",
    "clean:abi": "forge clean",
    "clean:js": "rimraf dist",
    "clean:mud": "rimraf src/codegen",
    "deploy:garnet": "mud deploy --profile=garnet",
    "deploy:local": "mud deploy --salt 0x",
    "deploy:testnet": "mud deploy --profile=lattice-testnet",
    "dev": "tsup --watch",
    "lint": "pnpm run prettier && pnpm run solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "test": "tsc --noEmit && mud test"
  },
  "dependencies": {
    "@latticexyz/cli": "workspace:*",
    "@latticexyz/schema-type": "workspace:*",
    "@latticexyz/store": "workspace:*",
    "@latticexyz/world": "workspace:*",
    "@latticexyz/world-modules": "workspace:*"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "0.7.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "prettier": "3.2.5",
    "prettier-plugin-solidity": "1.3.1",
    "solhint": "^3.3.7",
    "tsup": "^6.7.0"
  }
}
